div\r\n                  className={`${styles.button} bg-[#000] mt-4 rounded-[4px] h-11`}\r\n                  onClick={handleMessageSubmit}\r\n                >\r\n                  <span className=\"text-[#fff] flex items-center\">\r\n                    Gi tin nhn <AiOutlineMessage className=\"ml-1\" />\r\n                  </span>\r\n                </div>\r\n                {/* <h5 className=\"text-[16px] text-[red] mt-5\">(50)  bn</h5> */}\r\n              </div>\r\n\r\n              <div className=\"w-full 800px:w-[50%] pt-5 pl-[5px] pr-[5px]\">\r\n                <h1 className={`${styles.productTitle} text-[20px]`}>\r\n                  {data.name}\r\n                </h1>\r\n                <p className=\"py-2 text-[18px] leading-8 pb-10 whitespace-pre-line\">\r\n                  {showFullDescription\r\n                    ? data?.description\r\n                    : `${data?.description.substring(0, 250)}...`}\r\n                  <br />\r\n                  {!showFullDescription && (\r\n                    <button className=\"font-bold\" onClick={handleShowMoreClick}>\r\n                      Xem thm\r\n                    </button>\r\n                  )}\r\n                  <br />\r\n                  {showFullDescription && (\r\n                    <button className=\"font-bold\" onClick={handleShowLessClick}>\r\n                      Thu gn\r\n                    </button>\r\n                  )}\r\n                </p>\r\n\r\n                <div className=\"flex pt-3\">\r\n                  <h3 className={`${styles.price}`}>\r\n                    {data.sellPrice\r\n                      ? data.sellPrice.toLocaleString(\"vi-VN\", {\r\n                          style: \"currency\",\r\n                          currency: \"VND\",\r\n                        })\r\n                      : null}{\" \"}\r\n                    / {data.unit}\r\n                  </h3>\r\n                </div>\r\n                <div className=\"flex items-center mt-12 jus